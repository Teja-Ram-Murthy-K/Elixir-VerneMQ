{application,my_vernemq_plugin,
    [{applications,[kernel,stdlib,elixir,logger,httpoison,exjsx]},
     {description,"my_vernemq_plugin"},
     {modules,['Elixir.MyVMQPlugin']},
     {registered,[]},
     {vsn,"0.1.0"},
     {env,
         [{vmq_plugin_hooks,
              [{on_client_wakeup,'Elixir.MyVMQPlugin',on_client_wakeup,1,[]},
               {on_client_offline,'Elixir.MyVMQPlugin',on_client_offline,1,[]},
               {on_client_gone,'Elixir.MyVMQPlugin',on_client_gone,1,[]},
               {on_publish,'Elixir.MyVMQPlugin',on_publish,6,[]}]},
          {endpoint_on_client_wakeup,<<"http://localhost:1889">>},
          {endpoint_on_client_offline,<<"http://localhost:1889">>},
          {endpoint_on_client_gone,<<"http://localhost:1889">>},
          {endpoint_on_publish,<<"http://localhost:1889">>}]}]}.
